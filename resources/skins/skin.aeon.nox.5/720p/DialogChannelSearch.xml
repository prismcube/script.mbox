				<texturesliderbarfocus border="1">scrollbar/texturesliderbar2.png</texturesliderbarfocus>
				<onleft>400</onleft>
				<onright>402</onright>
				<ondown>104</ondown>
				<onup>104</onup>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
			</control>

			<control type="label">
				<posx>550</posx>
				<posy>467</posy>
				<width>300</width>
				<height>35</height>
				<font>font13</font>
				<align>right</align>
				<aligny>center</aligny>
				<scroll>true</scroll>
				<textcolor>grey</textcolor>
				<label>([COLOR=FF6db9e5]$INFO[Container(400).NumItems][/COLOR]) $ADDON[script.mbox 1106] - $ADDON[script.mbox 1107] ([COLOR=FF6db9e5]$INFO[Container(400).CurrentPage]/$INFO[Container(400).NumPages][/COLOR])</label>
				<visible>Control.IsVisible(400)</visible>
			</control>
			
