<window>
	<defaultcontrol always="true">104</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<control type="group" id="8000">
			<posx>40</posx>
			<posy>60</posy>
			<control type="image">
				<posx>0</posx>
				<posy>-30</posy>
				<width>1200</width>
				<height>700</height>
				<include>DialogBackground</include>
			</control>
			<control type="label" id="100">
				<description>Title</description>
				<posx>0</posx>
				<posy>-10</posy>
				<width>1200</width>
				<height>60</height>
				<font>font15_title</font>
				<textcolor>grey3</textcolor>
				<label>$ADDON[script.mbox 1017]</label>
				<align>center</align>
				<textoffsetx>45</textoffsetx>
			</control>
			<control type="button" id="104">
				<description>Close</description>
				<posx>1130</posx>
				<posy>10</posy>
				<width>20</width>
				<height>20</height>
				<texturefocus>close.png</texturefocus>
				<texturenofocus>close_nofocus.png</texturenofocus>
				<onleft>400</onleft>
				<onright>402</onright>
				<onup>400</onup>
				<ondown>400</ondown>
				<visible>system.getbool(input.enablemouse)</visible>
			</control>
			<control type="label" id="101">
				<description>TV Channels</description>
				<posx>85</posx>
				<posy>60</posy>
				<width>500</width>
				<height>40</height>
				<font>font13</font>
				<label>$ADDON[script.mbox 1018]</label>
				<align>left</align>
				<aligny>center</aligny>
				<textcolor>grey</textcolor>
			</control>
			<control type="label" id="103">
				<description>Radio Channels</description>
				<posx>620</posx>
				<posy>60</posy>
				<width>500</width>
				<height>40</height>
				<font>font13</font>
				<label>$ADDON[script.mbox 1019]</label>
				<align>left</align>
				<aligny>center</aligny>
				<textcolor>grey</textcolor>
			</control>
			<!-- TV List -->
			<control type="list" id="400">
				<posx>85</posx>
				<posy>100</posy>
				<width>470</width>
				<height>360</height>
				<onleft>403</onleft>
				<onright>401</onright>
				<onup>104</onup>
				<ondown>104</ondown>
				<scrolltime>200</scrolltime>
				<pagecontrol>401</pagecontrol>
				<itemlayout height="40" width="470">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>470</width>
						<height>41</height>
						<texture border="0,2,0,2">MenuItemNF.png</texture>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>0</posy>
						<width>400</width>
						<height>40</height>
						<font>Font_Reg17</font>
						<textcolor>grey3</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="image">
						<posx>430</posx>
						<posy>12</posy>
						<width>26</width>
						<height>18</height>
						<texture>IconCas1g.png</texture>
						<visible>substring(ListItem.Property(IsCas),True)</visible>
					</control>
				</itemlayout>
				<focusedlayout height="40" width="470">
					<control type="image">
						<animation effect="fade" start="100" end="20" time="100" condition="!Control.HasFocus(400)">Conditional</animation>
						<posx>0</posx>
						<posy>0</posy>
						<width>470</width>
						<height>41</height>
						<texture border="4">listselect_fo.png</texture>
						<colordiffuse>$VAR[FocusTextureColorVar]</colordiffuse>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>0</posy>
						<width>400</width>
						<height>40</height>
						<font>Font_Reg17</font>
						<textcolor>white</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="image">
						<posx>430</posx>
						<posy>12</posy>
						<width>26</width>
						<height>18</height>
						<texture>IconCas1w.png</texture>
						<visible>substring(ListItem.Property(IsCas),True)</visible>
					</control>
				</focusedlayout>
			</control>

