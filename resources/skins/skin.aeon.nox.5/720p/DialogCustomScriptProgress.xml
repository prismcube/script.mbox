<window>
	<allowoverlay>yes</allowoverlay>
	<defaultcontrol always="true">300</defaultcontrol>
	<controls>
		<control type="group">
			<posx>340</posx>
			<posy>195</posy>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>610</width>
				<height>320</height>
				<include>DialogBackground</include>
			</control>
			<control type="label" id="200">
				<description>Title</description>
				<posx>0</posx>
				<posy>3</posy>
				<width>610</width>
				<height>60</height>
				<font>font15_title</font>
				<textcolor>grey3</textcolor>
				<label>$ADDON[script.mbox 1126]</label>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>selected</textcolor>
			</control>
			<control type="button" id="301">
				<description>Close</description>
				<posx>550</posx>
				<posy>25</posy>
				<width>20</width>
				<height>20</height>
				<texturefocus>close.png</texturefocus>
				<texturenofocus>close_nofocus.png</texturenofocus>
				<onleft>300</onleft>
				<onright>300</onright>
				<onup>300</onup>
				<ondown>300</ondown>
			</control>
			<control type="label" id="201">
				<description>Message 1</description>
				<posx>30</posx>
				<posy>200</posy>
				<width>550</width>
				<height>30</height>
				<align>left</align>
				<aligny>center</aligny>
				<haspath>true</haspath>
				<font>font13</font>
			</control>
			<control type="textbox" id="202">
				<posx>30</posx>
				<posy>60</posy>
				<width>530</width>
				<height>120</height>
				<visible>true</visible>
				<colordiffuse>FFFFFFFF</colordiffuse>
				<font>font13</font>
				<textcolor>grey3</textcolor>
				<wrapmultiline>true</wrapmultiline>
				<label>$INFO[Window.Property(ShellDescription)]</label>
			</control>
			<control type="progress" id="400">
				<posx>30</posx>
				<posy>228</posy>
				<width>550</width>
				<height>16</height>
				<texturebg border="2">osd/texturebg2.png</texturebg>
			</control>
			<control type="button" id="300">
				<description>Cancel</description>
				<posx>215</posx>
				<posy>255</posy>
				<width>180</width>
				<height>35</height>
                <font>font14</font>
                <textcolor>grey2</textcolor>
                <focusedcolor>white</focusedcolor>
               	<textoffsetx>30</textoffsetx>
				<align>center</align>
				<onleft>301</onleft>
				<onright>301</onright>
				<onup>301</onup>
				<ondown>301</ondown>
			</control>
		</control>
	</controls>
</window>

