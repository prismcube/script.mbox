			</control>
			<control type="group">
				<control type="label">
					<posx>590</posx>
					<posy>567</posy>
					<width>300</width>
					<height>35</height>
					<font>font13</font>
					<align>right</align>
					<aligny>center</aligny>
					<scroll>true</scroll>
					<textcolor>grey</textcolor>
					<label>([COLOR=FF6db9e5]$INFO[Control.GetLabel(3903)]/$INFO[Container(1003950).NumItems][/COLOR]) $ADDON[script.mbox 1106] - $ADDON[script.mbox 1107] ([COLOR=FF6db9e5]$INFO[Container(1003950).CurrentPage]/$INFO[Container(1003950).NumPages][/COLOR])</label>
				</control>
			</control>
			<control type="button" id="3904">
				<posx>30</posx>
				<posy>575</posy>
				<width>200</width>
				<height>35</height>
				<label>$ADDON[script.mbox 1022]</label>
                <font>font14</font>
                <textcolor>grey2</textcolor>
				<texturenofocus border="5">button-nofocus.png</texturenofocus>
                <focusedcolor>white</focusedcolor>
                <align>center</align>
				<textoffsetx>30</textoffsetx>
				<onleft>1003950</onleft>
				<onright>1003951</onright>
				<onup>1003950</onup>
				<ondown>3901</ondown>
			</control>

