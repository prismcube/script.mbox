<window>
	<defaultcontrol always="true">102</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<include>DefaultDialogOpenEffect</include>
	<controls>
		<include>DefaultBehindDialogFadeOut</include>
		<control type="group" id="9000">
			<posx>600</posx>
			<posy>200</posy>
            <include>DefaultDialogCloseEffect</include>
			<control type="image">
				<description>background top image</description>
				<posx>0</posx>
				<posy>-54</posy>
				<width>370</width>
				<height>54</height>
				<texture border="45,45,45,0">confluence/DialogContextTop.png</texture>
			</control>
			<control type="button">
				<description>Close Window button</description>
				<posx>260</posx>
				<posy>-24</posy>
				<width>64</width>
				<height>32</height>
				<label>-</label>
				<font>-</font>
				<onclick>PreviousMenu</onclick>
				<texturefocus>confluence/DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>confluence/DialogCloseButton.png</texturenofocus>
			</control>
			<control type="image">
				<posx>34</posx>
				<posy>-6</posy>
				<width>297</width>
				<height>4</height>
				<texture>confluence/separator.png</texture>
			</control>
			<control type="image" id="100">
				<description>background image</description>
				<posx>0</posx>
				<posy>0</posy>
				<width>370</width>
				<texture border="45,0,45,0">confluence/DialogContextMiddle.png</texture>
			</control>
			<control type="image" id="101">
				<description>background bottom image</description>
				<posx>0</posx>
				<width>370</width>
				<height>50</height>
				<texture border="45,0,45,49">confluence/DialogContextBottom.png</texture>
			</control>
			<control type="list" id="102">
				<posx>34</posx>
				<posy>0</posy>
				<width>297</width>
				<height>500</height>
				<onup>102</onup>
				<ondown>102</ondown>
				<onleft>103</onleft>
				<onright>103</onright>
				<itemlayout height="38" width="297">
					<control type="label">
						<posx>150</posx>
						<posy>0</posy>
						<width>297</width>
						<height>38</height>
						<font>fontContextMenu</font>
						<align>center</align>
						<aligny>center</aligny>
						<textcolor>grey2</textcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>		
				</itemlayout>
				<focusedlayout height="38" width="297">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>297</width>
						<height>38</height>
						<texture border="5">confluence/button-focus.png</texture>
					</control>
					<control type="label">
						<posx>150</posx>
						<posy>0</posy>
						<width>297</width>
						<height>38</height>
						<font>fontContextMenu</font>
						<align>center</align>
						<aligny>center</aligny>
						<textcolor>white</textcolor>
						<label>$INFO[ListItem.Label]</label>
						<scroll>true</scroll>
					</control>
				</focusedlayout>
			</control>
		</control>
	</controls>
</window>
