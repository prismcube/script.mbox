<window>
	<defaultcontrol always="true">9001</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<include>DefaultDialogOpenEffect</include>
	<controls>
		<control type="group" id="9000">
			<visible>substring(Window.Property(DialogDrawFinished),True)</visible>
			<include>DefaultDialogCloseEffect</include>
			<posx>360</posx>
			<posy>50</posy>
			<!-- Panel -->
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>600</width>
				<height>565</height>
				<texture border="40">DialogBack.png</texture>
			</control>
			<control type="image">
				<posx>40</posx>
				<posy>10</posy>
				<width>600</width>
				<height>120</height>
				<aspectratio>stretch</aspectratio>
				<texture>GlassTitleBar.png</texture>
			</control>
			<control type="button" id="109">
				<description>Close Window</description>
				<posx>520</posx>
				<posy>9</posy>
				<width>64</width>
				<height>32</height>
				<font>-</font>
				<texturefocus>DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>DialogCloseButton.png</texturenofocus>
				<onleft>9001</onleft>
				<onright>8001</onright>
				<onup>100</onup>
				<ondown>9001</ondown>
			</control>
			<control type="label" id="101">
				<description>Title</description>
				<posx>20</posx>
				<posy>20</posy>
				<width>570</width>
				<height>30</height>
				<font>font13_title</font>
				<label>$INFO[Control.GetLabel]</label>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>white</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<control type="label">
				<posx>570</posx>
				<posy>480</posy>
				<width>300</width>
				<height>35</height>
				<font>font12</font>
				<align>right</align>
				<aligny>center</aligny>
				<scroll>true</scroll>
				<textcolor>grey</textcolor>
				<label>([COLOR=blue]$INFO[Window.Property(SelectedPosition)][/COLOR]) $ADDON[script.mbox 1106] - $ADDON[script.mbox 1107] ([COLOR=blue]$INFO[Container(9001).CurrentPage]/$INFO[Container(9001).NumPages][/COLOR])</label>
				<visible>Control.IsVisible(9001)</visible>
			</control>
			<control type="button" id="100">
				<posx>20</posx>
				<posy>490</posy>
				<width>200</width>
				<height>40</height>
				<label>$ADDON[script.mbox 1022]</label>
				<font>font12_title</font>
				<textcolor>white</textcolor>
				<focusedcolor>white</focusedcolor>
				<align>center</align>
				<texturenofocus>MenuItemNF.png</texturenofocus>
				<texturefocus>button-focus.png</texturefocus>
				<onleft>9001</onleft>
				<onright>8001</onright>
				<onup>9001</onup>
				<ondown>3800</ondown>
			</control>
			<!-- left key comment -->
			<control type="group">
				<posx>20</posx>
				<posy>530</posy>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>20</width>
					<height>20</height>
					<texture flipx="true">OSDPlayNF.png</texture>
				</control>
				<control type="label">
					<posx>20</posx>
					<posy>0</posy>
					<width>570</width>
					<height>30</height>
					<font>font10</font>
					<align>left</align>
					<scroll>true</scroll>
					<textcolor>grey3</textcolor>
					<shadowcolor>black</shadowcolor>
					<label>$ADDON[script.mbox 1138]</label>
				</control>
			</control>
			<control type="list" id="9001">
				<posx>30</posx>
				<posy>60</posy>
				<width>600</width>
				<height>420</height>
				<onleft>100</onleft>
				<onright>8001</onright>
				<onup>9001</onup>
				<ondown>9001</ondown>
				<pagecontrol>8001</pagecontrol>
				<scrolltime>200</scrolltime>
				<focusposition>0</focusposition>
				<movement>2</movement>
				<animation effect="slide" start="0,0" end="10,0" time="0" condition="!Control.IsVisible(8001)">Conditional</animation>
				<itemlayout height="70" width="590">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>530</width>
						<height>68</height>
						<texture border="5">button-nofocus.png</texture>
					</control>
					<!-- Selection Icon -->
					<control type="image">
						<posx>0</posx>
						<posy>5</posy>
						<width>32</width>
						<height>32</height>
						<texture>OverlayWatched.png</texture>
						<visible>substring(ListItem.Property(iMark),True)</visible>
					</control>
					<!-- Channel Name-->
					<control type="label">
						<posx>40</posx>
						<posy>0</posy>
						<width>360</width>
						<height>30</height>
						<font>font13_title</font>
						<textcolor>grey2</textcolor>
						<selectedcolor>selected</selectedcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<!-- Record Name-->
					<control type="label">
						<posx>40</posx>
						<posy>30</posy>
						<width>360</width>
						<height>30</height>
						<font>font13_title</font>
						<textcolor>grey2</textcolor>
						<selectedcolor>selected</selectedcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label2]</label>
					</control>
					<!-- Start Time -->
					<control type="label">
						<posx>500</posx>
						<posy>0</posy>
						<width>250</width>
						<height>30</height>
						<font>font13_title</font>
						<textcolor>grey2</textcolor>
						<selectedcolor>selected</selectedcolor>
						<align>right</align>
						<aligny>center</aligny>						
						<label>$INFO[ListItem.Property(StartTime)]</label>
					</control>
					<!-- Duration -->
					<control type="label">
						<posx>500</posx>
						<posy>30</posy>
						<width>200</width>
						<height>30</height>
						<font>font13_title</font>
						<textcolor>grey2</textcolor>
						<selectedcolor>selected</selectedcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Property(Duration)]</label>
					</control>
				</itemlayout>
				<focusedlayout height="70" width="590">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>530</width>
						<height>68</height>
						<texture border="5">StackFO_white2.png</texture>
						<include>DefaultVisibleFadeEffect</include>
					</control>
					<!-- Selection Icon -->
					<control type="image">
						<posx>0</posx>
						<posy>5</posy>
						<width>32</width>
						<height>32</height>
						<texture>OverlayWatched.png</texture>
						<visible>substring(ListItem.Property(iMark),True)</visible>
					</control>
					<!-- Channel Name-->
					<control type="label">
						<posx>40</posx>
						<posy>0</posy>
						<width>360</width>
						<height>30</height>
						<font>font13_title</font>
						<textcolor>white</textcolor>
						<selectedcolor>white</selectedcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<!-- Record Name-->
					<control type="label">
						<posx>40</posx>
						<posy>30</posy>
						<width>360</width>
						<height>30</height>
						<font>font13_title</font>
						<textcolor>white</textcolor>
						<selectedcolor>white</selectedcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label2]</label>
					</control>
					<!-- Start Time -->
					<control type="label">
						<posx>500</posx>
						<posy>0</posy>
						<width>250</width>
						<height>30</height>
						<font>font13_title</font>
						<textcolor>white</textcolor>
						<selectedcolor>white</selectedcolor>
						<align>right</align>
						<aligny>center</aligny>						
						<label>$INFO[ListItem.Property(StartTime)]</label>
					</control>
					<!-- Duration -->
					<control type="label">
						<posx>500</posx>
						<posy>30</posy>
						<width>200</width>
						<height>30</height>
						<font>font13_title</font>
						<textcolor>white</textcolor>
						<selectedcolor>white</selectedcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Property(Duration)]</label>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="8001">
				<posx>560</posx>
				<posy>60</posy>
				<width>25</width>
				<height>420</height>
				<include>DefaultWindowFade</include>
				<texturesliderbackground border="0,14,0,14">ScrollBarV.png</texturesliderbackground>
				<texturesliderbar border="0,14,0,14">ScrollBarV_bar.png</texturesliderbar>
				<texturesliderbarfocus border="0,14,0,14">ScrollBarV_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>ScrollBarNib.png</textureslidernib>
				<textureslidernibfocus>ScrollBarNib.png</textureslidernibfocus>
				<onleft>9001</onleft>
				<onright>9001</onright>
				<ondown></ondown>
				<onup></onup>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
			</control>
		</control>
	</controls>
</window>
