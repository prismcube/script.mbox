<window>
	<allowoverlay>yes</allowoverlay>
	<include>DefaultDialogOpenEffect</include>
	<defaultcontrol always="true">300</defaultcontrol>
	<controls>
		<control type="group">
			<posx>340</posx>
			<posy>195</posy>
			<include>DefaultDialogCloseEffect</include>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>610</width>
				<height>320</height>
				<texture border="40">DialogBack.png</texture>
			</control>
			<control type="image">
				<description>Dialog Header image</description>
				<posx>40</posx>
				<posy>16</posy>
				<width>530</width>
				<height>40</height>
				<texture>dialogheader.png</texture>
			</control>
			<control type="label" id="200">
				<description>header label</description>
				<posx>40</posx>
				<posy>20</posy>
				<width>530</width>
				<height>30</height>
				<font>font13_title</font>
				<label>SCRIPT1126</label>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>selected</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<control type="button" id="301">
				<description>Close Window button</description>
				<posx>520</posx>
				<posy>15</posy>
				<width>64</width>
				<height>32</height>
				<label>-</label>
				<font>-</font>
				<texturefocus>DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>DialogCloseButton.png</texturenofocus>
				<onleft>300</onleft>
				<onright>300</onright>
				<onup>300</onup>
				<ondown>300</ondown>
			</control>
			<control type="label" id="201">
				<description>dialog line 1</description>
				<posx>30</posx>
				<posy>200</posy>
				<width>550</width>
				<height>30</height>
				<align>left</align>
				<aligny>center</aligny>
				<haspath>true</haspath>
				<font>font13</font>
			</control>
			<control type="textbox" id="202">
				<posx>30</posx>
				<posy>60</posy>
				<width>535</width>
				<height>130</height>
				<visible>true</visible>
				<colordiffuse>FFFFFFFF</colordiffuse>
				<font>font12</font>
				<textcolor>grey3</textcolor>
				<wrapmultiline>true</wrapmultiline>
				<label>$INFO[Window.Property(ShellDescription)]</label>
			</control>
			<control type="progress" id="400">
				<description>Progressbar</description>
				<posx>30</posx>
				<posy>228</posy>
				<width>550</width>
				<height>16</height>
				<texturebg border="6,0,6,0">OSDProgressBack.png</texturebg>
				<midtexture border="6,0,6,0">OSDProgressMid.png</midtexture>
			</control>
			<control type="button" id="300">
				<description>Cancel button</description>
				<posx>205</posx>
				<posy>255</posy>
				<width>200</width>
				<height>40</height>
				<font>font12_title</font>
				<textcolor>white</textcolor>
				<focusedcolor>white</focusedcolor>
				<align>center</align>
				<onleft>301</onleft>
				<onright>301</onright>
				<onup>301</onup>
				<ondown>301</ondown>
			</control>
		</control>
	</controls>
</window>

