<window id="3033">
	<defaultcontrol always="false">49</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<include>DefaultSettingHeaderWindow</include>
		<include>DefaultSettingWindowPigScreen</include>
		<control type="group">
			<posx>80</posx>
			<posy>110</posy>
			<include>DefaultSettingWindowEffect</include>
			<include>DefaultSettingMainWindow</include>
			<control type="group" id="49">
				<posx>10</posx>
				<include>DefaultWindowFade</include>
				<control type="list" id="50">
					<posx>0</posx>
					<posy>50</posy>
					<width>800</width>
					<height>521</height>
					<onleft>61</onleft>
					<onright>61</onright>
					<onup>50</onup>
					<ondown>50</ondown>
					<pagecontrol>61</pagecontrol>
					<scrolltime>200</scrolltime>
					<focusposition>0</focusposition>
					<movement>2</movement>
					<itemlayout height="40" width="800">
						<control type="image">
							<posx>0</posx>
							<posy>0</posy>
							<width>700</width>
							<height>41</height>
							<texture border="0,2,0,2">MenuItemNF.png</texture>
						</control>
						<control type="label">
							<posx>30</posx>
							<posy>0</posy>
							<width>660</width>
							<height>40</height>
							<font>font13</font>
							<textcolor>grey3</textcolor>
							<align>left</align>
							<aligny>center</aligny>
							<label>$INFO[ListItem.Label]</label>
							<animation effect="slide" start="0,0" end="16,0" delay="0" time="0" condition="false">conditional</animation>
						</control>
						<control type="label">
							<posx>700</posx>
							<posy>0</posy>
							<width>200</width>
							<height>40</height>
							<font>font13</font>
							<textcolor>grey2</textcolor>
							<selectedcolor>selected</selectedcolor>
							<align>right</align>
							<aligny>center</aligny>
							<label>$INFO[ListItem.Label2]</label>
						</control>	
					</itemlayout>
					<focusedlayout height="40" width="800">
						<control type="image">
							<posx>0</posx>
							<posy>0</posy>
							<width>700</width>
							<height>41</height>
							<texture border="0,2,0,2">MenuItemNF.png</texture>
							<animation effect="fade" time="300">Visible</animation>
							<animation effect="fade" time="300">Hidden</animation>
						</control>
						<control type="image">
							<posx>0</posx>
							<posy>0</posy>
							<width>700</width>
							<height>41</height>
							<visible>!Control.IsVisible(600)</visible>
							<texture border="0,2,0,2">MenuItemFO.png</texture>
						</control>
						<control type="label">
							<posx>30</posx>
							<posy>0</posy>
							<width>660</width>
							<height>40</height>
							<font>font13</font>
							<textcolor>white</textcolor>
							<selectedcolor>selected</selectedcolor>						
							<align>left</align>
							<aligny>center</aligny>
							<label>$INFO[ListItem.Label]</label>
						</control>
						<control type="label">
							<posx>700</posx>
							<posy>0</posy>
							<width>200</width>
							<height>40</height>
							<font>font13</font>
							<textcolor>grey2</textcolor>
							<selectedcolor>selected</selectedcolor>
							<align>right</align>
							<aligny>center</aligny>
							<label>$INFO[ListItem.Label2]</label>
						</control>
					</focusedlayout>
				</control>
			</control>
			<!-- Scrollbar -->
			<control type="scrollbar" id="61">
				<posx>770</posx>
				<posy>125</posy>
				<width>25</width>
				<height>521</height>
				<include>DefaultWindowFade</include>
				<texturesliderbackground border="0,14,0,14">ScrollBarV.png</texturesliderbackground>
				<texturesliderbar border="0,14,0,14">ScrollBarV_bar.png</texturesliderbar>
				<texturesliderbarfocus border="0,14,0,14">ScrollBarV_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>ScrollBarNib.png</textureslidernib>
				<textureslidernibfocus>ScrollBarNib.png</textureslidernibfocus>
				<onleft>49</onleft>
				<onright>49</onright>
				<ondown>61</ondown>
				<onup>61</onup>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
			</control>

		</control>

		<control type="group">
			<posx>860</posx>
			<posy>370</posy>
			<control type="label" id="100">
				<posx>0</posx>
				<posy>0</posy>
				<font>font13_title</font>
				<textcolor>grey</textcolor>
				<align>left</align>
				<label>$INFO[Control.GetLabel]</label>
			</control>
			<control type="label" id="101">
				<posx>0</posx>
				<posy>25</posy>
				<font>font13_title</font>
				<textcolor>grey</textcolor>
				<align>left</align>
				<label>$INFO[Control.GetLabel]</label>
			</control>
			<control type="label" id="102">
				<posx>0</posx>
				<posy>50</posy>
				<font>font13_title</font>
				<textcolor>grey</textcolor>
				<align>left</align>
				<label>$INFO[Control.GetLabel]</label>
			</control>
			<control type="label">
				<posx>0</posx>
				<posy>75</posy>
				<font>font13_title</font>
				<textcolor>grey</textcolor>
				<align>left</align>
				<label>$INFO[Window.Property(DescriptionTitle)]</label>
			</control>
			<control type="textbox" id="103">
				  <posx>20</posx>
				  <posy>100</posy>
				  <width>300</width>
				  <height>100</height>
				  <visible>true</visible>
				  <colordiffuse>FFFFFFFF</colordiffuse>
				  <font>font12</font>
				  <textcolor>FFFFFFFF</textcolor>
				  <wrapmultiline>true</wrapmultiline>
				  <autoscroll delay="3000" time="1000" repeat="10000">true</autoscroll>
				  <label>$INFO[Window.Property(UpdateDescription)]</label>
			</control>

		</control>

	</controls>
</window>

