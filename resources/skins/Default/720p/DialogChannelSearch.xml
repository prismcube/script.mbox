<window>
	<defaultcontrol always="true">400</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<include>DefaultDialogOpenEffect</include>
	<controls>
		<control type="group">
			<posx>90</posx>
			<posy>40</posy>
			<include>DefaultDialogCloseEffect</include>
			<control type="image">
				<description>background image</description>
				<posx>0</posx>
				<posy>0</posy>
				<width>1100</width>
				<height>640</height>
				<texture border="40">DialogBack.png</texture>
			</control>
			<control type="image">
				<posx>40</posx>
				<posy>10</posy>
				<width>1020</width>
				<height>90</height>
				<aspectratio>stretch</aspectratio>
				<texture>GlassTitleBar.png</texture>
			</control>
			<control type="button" id="104">
				<description>Close Window button</description>
				<posx>1010</posx>
				<posy>9</posy>
				<width>64</width>
				<height>32</height>
				<label>-</label>
				<font>-</font>
				<onclick>-</onclick>
				<texturefocus>DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>DialogCloseButton.png</texturenofocus>
				<onleft>-</onleft>
				<onright>-</onright>
				<onup>-</onup>
				<ondown>-</ondown>
				<visible>system.getbool(input.enablemouse)</visible>
			</control>
			<control type="label" id="100">
				<description>Title</description>
				<posx>0</posx>
				<posy>10</posy>
				<width>1100</width>
				<height>40</height>
				<font>font13_title</font>
				<label>Channel Search Progress</label>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>white</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<control type="label" id="101">
				<description>TV List</description>
				<posx>55</posx>
				<posy>60</posy>
				<width>500</width>
				<height>40</height>
				<font>font13_title</font>
				<label>TV List</label>
				<align>left</align>
				<aligny>center</aligny>
				<textcolor>white</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<control type="label" id="103">
				<description>Radio List</description>
				<posx>570</posx>
				<posy>60</posy>
				<width>500</width>
				<height>40</height>
				<font>font13_title</font>
				<label>Radio List</label>
				<align>left</align>
				<aligny>center</aligny>
				<textcolor>white</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<!-- TV List -->
			<control type="list" id="400">
				<posx>55</posx>
				<posy>100</posy>
				<width>450</width>
				<height>360</height>
				<onleft>403</onleft>
				<onright>401</onright>
				<onup>400</onup>
				<ondown>400</ondown>
				<scrolltime>200</scrolltime>
				<pagecontrol>401</pagecontrol>
				<itemlayout height="40" width="500">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>449</width>
						<height>41</height>
						<texture border="0,2,0,2">MenuItemNF.png</texture>
					</control>
					<control type="label">
						<posx>0</posx>
						<posy>0</posy>
						<width>450</width>
						<height>40</height>
						<font>font13</font>
						<textcolor>grey3</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="label">
						<posx>450</posx>
						<posy>0</posy>
						<width>450</width>
						<height>40</height>
						<font>font13</font>
						<textcolor>grey2</textcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label2]</label>
					</control>	
				</itemlayout>
				<focusedlayout height="40" width="500">
					<control type="image">
						<animation effect="fade" start="100" end="20" time="100" condition="!Control.HasFocus(400)">Conditional</animation>
						<posx>0</posx>
						<posy>0</posy>
						<width>499</width>
						<height>41</height>
						<texture border="0,2,0,2">MenuItemFO.png</texture>
					</control>
					<control type="label">
						<posx>0</posx>
						<posy>0</posy>
						<width>450</width>
						<height>40</height>
						<font>font13</font>
						<textcolor>white</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="label">
						<posx>450</posx>
						<posy>0</posy>
						<width>450</width>
						<height>40</height>
						<font>font12</font>
						<textcolor>grey2</textcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label2]</label>
					</control>
				</focusedlayout>
			</control>

            <!-- TV Scrollbar -->
			<control type="scrollbar" id="401">
				<posx>510</posx>
				<posy>95</posy>
				<width>25</width>
				<height>375</height>
				<texturesliderbackground border="0,14,0,14">ScrollBarV.png</texturesliderbackground>
				<texturesliderbar border="0,14,0,14">ScrollBarV_bar.png</texturesliderbar>
				<texturesliderbarfocus border="0,14,0,14">ScrollBarV_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>ScrollBarNib.png</textureslidernib>
				<textureslidernibfocus>ScrollBarNib.png</textureslidernibfocus>
				<onleft>400</onleft>
				<onright>402</onright>
				<ondown>300</ondown>
				<onup>104</onup>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
			</control>

			<control type="label">
				<posx>510</posx>
				<posy>467</posy>
				<width>300</width>
				<height>35</height>
				<font>font12</font>
				<align>right</align>
				<aligny>center</aligny>
				<scroll>true</scroll>
				<textcolor>grey</textcolor>
				<label>([COLOR=blue]$INFO[Container(400).NumItems][/COLOR]) Items - Page([COLOR=blue]$INFO[Container(400).CurrentPage]/$INFO[Container(400).NumPages][/COLOR])</label>
				<visible>Control.IsVisible(400)</visible>
			</control>
			
			<!-- Radio List -->
			<control type="list" id="402">
				<posx>570</posx>
				<posy>100</posy>
				<width>450</width>
				<height>360</height>
				<onleft>401</onleft>
				<onright>403</onright>
				<onup>402</onup>
				<ondown>402</ondown>
				<pagecontrol>403</pagecontrol>
				<itemlayout height="40" width="500">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>449</width>
						<height>41</height>
						<texture border="0,2,0,2">MenuItemNF.png</texture>
