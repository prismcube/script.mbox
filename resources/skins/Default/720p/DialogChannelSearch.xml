			<control type="label" id="404">
				<description>TransponderInfo</description>
				<posx>55</posx>
				<posy>510</posy>
				<width>1000</width>
				<height>40</height>
				<font>font13_title</font>
				<label>SCRIPT20</label>
				<align>left</align>
