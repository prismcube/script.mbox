				<textcolor>white</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<!-- Progress Bar -->
			<control type="progress" id="200">
				<description>Progressbar</description>
				<posx>55</posx>
				<posy>560</posy>
				<width>1090</width>
				<height>30</height>
			</control>
		</control>
	</controls>
</window>
