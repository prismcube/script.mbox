				<onleft>402</onleft>
				<onright>400</onright>
				<ondown>104</ondown>
				<onup>104</onup>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
			</control>

			<control type="label">
				<posx>1062</posx>
				<posy>467</posy>
				<width>300</width>
				<height>35</height>
				<font>font12</font>
				<align>right</align>
				<aligny>center</aligny>
				<scroll>true</scroll>
				<textcolor>grey</textcolor>
				<label>([COLOR=blue]$INFO[Container(402).NumItems][/COLOR]) SCRIPT106 - SCRIPT107 ([COLOR=blue]$INFO[Container(402).CurrentPage]/$INFO[Container(402).NumPages][/COLOR])</label>
				<visible>Control.IsVisible(402)</visible>
			</control>

