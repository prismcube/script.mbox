<window>
	<defaultcontrol always="true">9000</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<include>DefaultDialogOpenEffect</include>
	<controls>
		<control type="group">
			<visible>substring(Window.Property(DialogDrawFinished),True)</visible>
			<include>DefaultDialogCloseEffect</include>
			<posx>335</posx>
			<posy>160</posy>
			<!-- Panel -->
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>610</width>
				<height>400</height>
				<texture border="40">DialogBack.png</texture>
			</control>
			<control type="image">
				<posx>40</posx>
				<posy>10</posy>
				<width>530</width>
				<height>90</height>
				<aspectratio>stretch</aspectratio>
				<texture>GlassTitleBar.png</texture>
			</control>
			<control type="button" id="300">
				<description>Close Window</description>
				<posx>520</posx>
				<posy>9</posy>
				<width>64</width>
				<height>32</height>
				<label>-</label>
				<font>-</font>
				<texturefocus>DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>DialogCloseButton.png</texturenofocus>
				<onleft>9000</onleft>
				<onright>9000</onright>
				<onup>9000</onup>
				<ondown>9000</ondown>
			</control>
			<control type="label" id="301">
				<description>Title</description>
				<posx>80</posx>
				<posy>21</posy>
				<width>440</width>
				<height>30</height>
				<font>font13_title</font>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>white</textcolor>
				<shadowcolor>black</shadowcolor>
				<scroll>true</scroll>
			</control>
			<control type="list" id="9000">
				<posx>15</posx>
				<posy>65</posy>
				<width>550</width>
				<height>240</height>
				<onup>9000</onup>
				<ondown>9000</ondown>
				<onleft>300</onleft>
				<onright>400</onright>
				<pagecontrol>400</pagecontrol>
				<scrolltime>200</scrolltime>
				<animation effect="slide" start="0,0" end="10,0" time="0" condition="!Control.IsVisible(61)">Conditional</animation>
				<itemlayout height="40" width="550">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>550</width>
						<height>41</height>
						<texture border="0,2,0,2">MenuItemNF.png</texture>
					</control>
					<control type="label">
						<posx>15</posx>
						<posy>0</posy>
						<width>510</width>
						<height>40</height>
						<font>font13</font>
						<textcolor>grey2</textcolor>
						<selectedcolor>selected</selectedcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</itemlayout>
				<focusedlayout height="40" width="550">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>550</width>
						<height>41</height>
						<texture border="0,2,0,2">MenuItemNF.png</texture>
						<visible>!Control.HasFocus(9000)</visible>
						<include>VisibleFadeEffect</include>
					</control>
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>550</width>
						<height>41</height>
						<texture border="0,2,0,2">MenuItemFO.png</texture>
						<visible>Control.HasFocus(9000)</visible>
						<include>VisibleFadeEffect</include>
					</control>
					<control type="label">
						<posx>15</posx>
						<posy>0</posy>
						<width>510</width>
						<height>40</height>
						<font>font13</font>
						<textcolor>white</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="400">
				<posx>570</posx>
				<posy>60</posy>
				<width>25</width>
				<height>250</height>
				<texturesliderbackground border="0,14,0,14">ScrollBarV.png</texturesliderbackground>
				<texturesliderbar border="0,14,0,14">ScrollBarV_bar.png</texturesliderbar>
				<texturesliderbarfocus border="0,14,0,14">ScrollBarV_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>ScrollBarNib.png</textureslidernib>
				<textureslidernibfocus>ScrollBarNib.png</textureslidernibfocus>
				<onleft>9000</onleft>
				<onright>9000</onright>
				<ondown>400</ondown>
				<onup>400</onup>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
			</control>
			<control type="label" id="302">
				<posx>580</posx>
				<posy>310</posy>
				<width>540</width>
				<height>35</height>
				<font>font12</font>
				<align>right</align>
				<aligny>center</aligny>
				<scroll>true</scroll>
				<textcolor>grey</textcolor>
			</control>
			<control type="label" id="303">
				<posx>580</posx>
				<posy>340</posy>
				<width>540</width>
				<height>35</height>
				<font>font12</font>
				<align>right</align>
				<aligny>center</aligny>
				<scroll>true</scroll>
				<textcolor>grey</textcolor>
			</control>
		</control>
	</controls>
</window>
