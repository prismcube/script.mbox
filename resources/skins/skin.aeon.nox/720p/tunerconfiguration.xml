<window id="3007">
	<defaultcontrol always="true">9000</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<include>DefaultSettingHeaderWindow</include>	
		<control type="group">
			<include>DefaultSettingWindowEffect</include>
			<include>DefaultSettingMainWindow</include>
			<control type="list" id="9000">
				<posx>90</posx>
				<posy>150</posy>
				<width>750</width>
				<height>450</height>
				<itemgap>0</itemgap>
				<onup>9000</onup>
				<ondown>9000</ondown>
				<!--<pagecontrol>60</pagecontrol>-->
				<itemlayout height="40" width="720">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>750</width>
						<height>40</height>
						<texture>settings/settings_radiobuttonoff.png</texture>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>0</posy>
						<width>680</width>
						<height>40</height>
						<font>Font_Reg20</font>
						<textcolor>grey2</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</itemlayout>
				<focusedlayout height="40" width="720">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>750</width>
						<height>40</height>
						<texture>settings/settings_radiobuttonofffocus.png</texture>
					</control>
					<control type="label">
						<posx>10</posx>
						<posy>0</posy>
						<width>680</width>
						<height>40</height>
						<font>Font_Reg20</font>
						<textcolor>white</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
				</focusedlayout>
			</control>
		</control>
	</controls>
</window>