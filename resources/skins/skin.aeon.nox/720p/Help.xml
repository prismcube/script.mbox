<window id="3033">
	<defaultcontrol always="true">7001</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<onload>SetProperty(HelpString,Help_String.xml)</onload>	
	<controls>
		<control type="group">
			<control type="group" id="9000">
				<animation condition="Control.HasFocus(999)" type="Conditional" reversible="false">
					<effect type="zoom" start="100" end="80" center="640,360" easing="in" tween="back" time="300" />
					<effect type="fade" start="100" end="0" time="300" />
				</animation>
				<animation condition="!Control.HasFocus(999)" type="Conditional" reversible="false">
					<effect type="zoom" start="80" end="100" center="640,360" easing="out" tween="back" time="300" />
					<effect type="fade" start="0" end="100" time="300" />
				</animation>
				<include>DefaultSettingWindowEffect</include>

				<!-- Headers -->
				<control type="group">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>1280</width>
						<height>710</height>
						<texture border="20">DialogBack.png</texture>
					</control>
					<control type="label" id="1002">
						<description>header label</description>
						<posx>40</posx>
						<posy>30</posy>
						<width>900</width>
						<height>30</height>
						<font>Font_Reg25_Caps</font>
						<align>left</align>
						<aligny>center</aligny>
						<textcolor>white</textcolor>
						<shadowcolor>black</shadowcolor>
					</control>
					<control type="label" id="1003">
						<posx>40</posx>
						<posy>60</posy>
						<width>900</width>
						<height>30</height>
						<font>Font_Reg19</font>
						<textcolor>blue</textcolor>
						<align>left</align>
						<aligny>center</aligny>
					</control>
				</control>

				<!-- Contents -->
				<control type="group" id="8500">
					<control type="image" id="100">
						<texture border="20">$INFO[Window.Property(imagepath)]</texture>
						<visible>Control(8500).IsVisible()</visible>
					</control>
					<control type="textbox" id="200">
						<font>Font_Reg17</font>
						<textcolor>white</textcolor>
						<label>$INFO[Window.Property(label)]</label>
						<visible>Control(8500).IsVisible()</visible>
					</control>
					<control type="textbox" id="201">
						<font>Font_Reg17</font>
						<textcolor>white</textcolor>
						<label>$INFO[Window.Property(label1)]</label>
						<visible>Control(8500).IsVisible()</visible>
					</control>
					<control type="textbox" id="202">
						<font>Font_Reg17</font>
						<textcolor>white</textcolor>
						<label>$INFO[Window.Property(label2)]</label>
						<visible>Control(8500).IsVisible()</visible>
					</control>
				</control>
				
				<!-- Next & Previous Buttons -->
				<control type="group">
					<posx>250</posx>
					<posy>160</posy>
					<control type="button" id="7000">
						<description>Prev button</description>
						<posx>14</posx>
						<posy>485</posy>
						<width>30</width>
						<height>30</height>
						<texturenofocus border="5">views/multiplex_arrow2.png</texturenofocus>
						<texturefocus border="5">aeon.nox/multiplex_arrowfocus2.png</texturefocus>
						<onleft>7001</onleft>
						<onright>7001</onright>
					</control>
					<control type="label" id="7005">
						<posx>60</posx>
						<posy>478</posy>
						<width>200</width>
						<height>40</height>
						<font>font12_title</font>
						<textcolor>grey2</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<visible>Control.IsVisible(7000) + !Control.HasFocus(7000)</visible>
					</control>
					<control type="label">
						<posx>60</posx>
						<posy>478</posy>
						<width>200</width>
						<height>40</height>
						<font>font13_title</font>
						<textcolor>white</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[Control.GetLabel(7005)]</label>
						<visible>Control.IsVisible(7000) + Control.HasFocus(7000)</visible>
					</control>
					<control type="button" id="7001">
						<description>Next button</description>
						<posx>671</posx>
						<posy>485</posy>
						<width>30</width>
						<height>30</height>
						<texturenofocus border="5">views/multiplex_arrow.png</texturenofocus>
						<texturefocus border="5">aeon.nox/multiplex_arrowfocus.png</texturefocus>
						<onleft>7000</onleft>
						<onright>7000</onright>
					</control>
					<control type="label" id="7006">
						<posx>620</posx>
						<posy>478</posy>
						<width>200</width>
						<height>40</height>
						<font>font12_title</font>
						<textcolor>grey2</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<visible>Control.IsVisible(7001) + !Control.HasFocus(7001)</visible>
					</control>
					<control type="label">
						<posx>620</posx>
						<posy>478</posy>
						<width>200</width>
						<height>40</height>
						<font>font13_title</font>
						<textcolor>white</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[Control.GetLabel(7006)]</label>
						<visible>Control.IsVisible(7001) + Control.HasFocus(7001)</visible>
					</control>
				</control>

				<!-- Page Number -->
				<control type="label" id="1004">
					<posx>598</posx>
					<posy>645</posy>
					<width>100</width>
					<height>30</height>
					<font>font12_title</font>
					<textcolor>blue</textcolor>
					<align>left</align>
					<aligny>center</aligny>
				</control>
			</control>
		</control>
		<include>DefaultFakeButton</include>
	</controls>
</window>
