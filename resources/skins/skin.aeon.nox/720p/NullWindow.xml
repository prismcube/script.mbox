<window id="3001">
	<defaultcontrol always="true">9000</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<backgroundcolor>0x00ffffff</backgroundcolor>
	<controls>
		<control type="label">
			<posx>550</posx>
			<posy>330</posy>
			<width>500</width>
			<font>Font_Reg38</font>
			<label>[I][B]No Signal [/B][/I]</label>
			<textcolor>grey2</textcolor>
			<visible>substring(Window.Property(Signal),False)</visible>
		</control>
		<control type="label">
			<posx>550</posx>
			<posy>330</posy>
			<width>500</width>
			<font>Font_Reg38</font>
			<label>[I][B]Scrambled [/B][/I]</label>
			<textcolor>grey2</textcolor>
			<visible>substring(Window.Property(Signal),Scramble)</visible>
		</control>
	</controls>
</window>
