<window>
	<allowoverlay>yes</allowoverlay>
	<include>DefaultDialogOpenEffect</include>
	<controls>
		<control type="group" id="3900">
			<visible>substring(Window.Property(DialogDrawFinished),True)</visible>
			<posx>375</posx>
			<posy>60</posy>
			<control type="image">
				<posx>0</posx>
				<posy>-20</posy>
				<width>530</width>
				<height>625</height>
				<texture>dialogs/dialog_back.png</texture>
				<colordiffuse>$VAR[DialogColorVar]</colordiffuse>
			</control>
			<control type="image">
				<posx>20</posx>
				<width>490</width>
				<height>43</height>
				<texture>osd/osd_settings_buttonnf.png</texture>
			</control>
			<control type="label" id="3902">
				<description>Title</description>
				<posx>0</posx>
				<posy>0</posy>
				<width>530</width>
				<height>60</height>
				<font>Font_Reg19</font>
				<textcolor>FF6db9e5</textcolor>
				<align>center</align>
				<scroll>true</scroll>
				<textoffsetx>45</textoffsetx>
			</control>
			<control type="button" id="3901">
				<description>Close</description>
				<posx>470</posx>
				<posy>20</posy>
				<width>20</width>
				<height>20</height>
				<texturefocus>special://skin/media/close.png</texturefocus>
				<texturenofocus>close_nofocus.png</texturenofocus>
				<onleft>1003950</onleft>
				<onright>1003951</onright>
				<onup>3904</onup>
				<ondown>1003950</ondown>
			</control>

			<control type="label" id="3903">
				<textcolor>grey2</textcolor>
				<label>$INFO[Control.GetLabel]</label>
				<visible>false</visible>
			</control>

			<!-- Channels -->
			<include>DefaultSelectGroup</include>

			<!-- Item & Page Number -->
			<control type="label">
				<posx>500</posx>
				<posy>468</posy>
				<width>300</width>
				<height>35</height>
				<font>Font_Reg17</font>
				<align>right</align>
				<aligny>center</aligny>
				<scroll>true</scroll>
				<textcolor>grey</textcolor>
				<label>([COLOR=FF6db9e5]$INFO[Control.GetLabel(3903)]/$INFO[Container(1003960).NumItems][/COLOR]) $LOCALIZE[SCRIPT1106] - $LOCALIZE[SCRIPT1107] ([COLOR=FF6db9e5]$INFO[Container(1003960).CurrentPage]/$INFO[Container(1003960).NumPages][/COLOR])</label>
				<visible>substring(Window.Property(ZappingModeInfo),true)</visible>
			</control>
			<control type="label">
				<posx>500</posx>
				<posy>500</posy>
				<width>300</width>
				<height>35</height>
				<font>Font_Reg17</font>
				<align>right</align>
				<aligny>center</aligny>
				<scroll>true</scroll>
				<textcolor>grey</textcolor>
				<label>([COLOR=FF6db9e5]$INFO[Control.GetLabel(3903)]/$INFO[Container(1003960).NumItems][/COLOR]) $LOCALIZE[SCRIPT1106] - $LOCALIZE[SCRIPT1107] ([COLOR=FF6db9e5]$INFO[Container(1003960).CurrentPage]/$INFO[Container(1003960).NumPages][/COLOR])</label>
				<visible>!substring(Window.Property(ZappingModeInfo),true)</visible>
			</control>

			<control type="button" id="3904">
				<description>OK</description>
				<posx>20</posx>
				<posy>485</posy>
				<width>200</width>
				<height>35</height>
				<label>SCRIPT1022</label>
				<align>center</align>
                <colordiffuse>$VAR[ColorDiffuseVar]</colordiffuse>
                <font>Font_Reg19</font>
                <textcolor>grey2</textcolor>
                <focusedcolor>white3</focusedcolor>
				<onleft>1003950</onleft>
				<onright>1003951</onright>
				<onup>1003950</onup>
				<ondown>3901</ondown>
				<visible>false</visible>
			</control>

			<!-- Hotkey -->
			<control type="group">
				<posx>20</posx>
				<posy>540</posy>
				<visible>Control.IsVisible(3904)</visible>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>20</width>
					<height>20</height>
					<texture flipx="true">OSDPlayNF.png</texture>
				</control>
				<control type="label">
					<posx>20</posx>
					<posy>0</posy>
					<width>470</width>
					<height>30</height>
					<font>Font_Reg14</font>
					<align>left</align>
					<scroll>true</scroll>
					<textcolor>grey3</textcolor>
					<label>SCRIPT1138</label>
				</control>
			</control>

			<!-- Color Keys -->
			<control type="group">
				<posx>20</posx>
				<posy>510</posy>
				<visible>substring(Window.Property(ZappingModeInfo),true)</visible>
				<control type="group">
					<posx>0</posx>
					<posy>0</posy>
					<control type="image">
						<posx>0</posx>
						<posy>3</posy>
						<width>15</width>
						<height>15</height>
						<colordiffuse>FFFF0000</colordiffuse>
						<texture>Logo_Background.png</texture>
					</control>
					<control type="label">
						<posx>20</posx>
						<posy>0</posy>
						<width>100</width>
						<height>15</height>
						<font>Font_Reg14</font>
						<align>left</align>
						<label>Satellite</label>
					</control>
				</control>
				<control type="group">
					<posx>120</posx>
					<posy>0</posy>
					<control type="image">
						<posx>0</posx>
						<posy>3</posy>
						<width>15</width>
						<height>15</height>
						<colordiffuse>FF00FF00</colordiffuse>
						<texture>Logo_Background.png</texture>
					</control>
					<control type="label">
						<posx>20</posx>
						<posy>0</posy>
						<width>100</width>
						<height>15</height>
						<font>Font_Reg14</font>
						<align>left</align>
						<label>FTA/CAS</label>
					</control>
				</control>
				<control type="group">
					<posx>240</posx>
					<posy>0</posy>
					<control type="image">
						<posx>0</posx>
						<posy>3</posy>
						<width>15</width>
						<height>15</height>
						<colordiffuse>FFFFFF00</colordiffuse>
						<texture>Logo_Background.png</texture>
					</control>
					<control type="label">
						<posx>20</posx>
						<posy>0</posy>
						<width>100</width>
						<height>15</height>
						<font>Font_Reg14</font>
						<align>left</align>
						<label>Provider</label>
					</control>
				</control>
				<control type="group">
					<posx>360</posx>
					<posy>0</posy>
					<control type="image">
						<posx>0</posx>
						<posy>3</posy>
						<width>15</width>
						<height>15</height>
						<colordiffuse>FF0050FF</colordiffuse>
						<texture>Logo_Background.png</texture>
					</control>
					<control type="label">
						<posx>20</posx>
						<posy>0</posy>
						<width>100</width>
						<height>15</height>
						<font>Font_Reg14</font>
						<align>left</align>
						<label>Favorite</label>
					</control>
				</control>
			</control>
		</control>
	</controls>
</window>
