						<textcolor>grey3</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<control type="image">
						<posx>430</posx>
						<posy>12</posy>
						<width>26</width>
						<height>18</height>
						<texture>IconCas1g.png</texture>
						<visible>substring(ListItem.Property(IsCas),True)</visible>
					</control>
				</itemlayout>
