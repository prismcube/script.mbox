<window id="3027">
	<defaultcontrol always="true">9003</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<!-- Background -->
		<control type="group">
			<include>DefaultSettingWindowEffect</include>
			<control type="multiimage">
				<posx>0</posx>
				<posy>0</posy>
				<width>1280</width>
				<height>720</height>
				<imagepath>special://skin/backgrounds/curtains.jpg</imagepath>
				<timeperimage>5000</timeperimage>
				<fadetime>2000</fadetime>
				<pauseatend>10000</pauseatend>
				<randomize>true</randomize>
				<loop>no</loop>
			</control>
			<!-- PIP -->
			<control type="image" id="8899">
				<posx>862</posx>
				<posy>133</posy>
				<width>352</width>
				<height>198</height>
				<texture>video_pig_overlay.png</texture>
			</control>
		</control>

		<!-- Bottom Gradation Image -->
		<include>Furniture_Showcase</include>

		<!-- Channel Name on TOP side -->
		<control type="label" id="400">
			<description>Channel Name</description>					
			<posx>100</posx>
			<posy>15</posy>
			<width>600</width>
			<height>40</height>
			<font>font13_title</font>
			<textcolor>blue</textcolor>
			<align>left</align>
			<aligny>center</aligny>
			<label></label>
			<visible>substring(Window.Property(EPGMode),channel) </visible>			
		</control>

		<!-- Left big Box -->
		<control type="group">
			<control type="image">
				<posx>394</posx>
				<posy>655</posy>
				<width>1280</width>
				<height>68</height>
				<texture background="true">views/showcase_gradient.png</texture>
				<include>Animation_CommonFade</include>
				<include>Animation_HideMultiplex</include>
			</control>
			<control type="group">
				<animation effect="slide" time="700" start="-800,0" end="0,0" tween="cubic" easing="out">WindowOpen</animation>
				<animation effect="slide" time="700" start="0,0" end="-800,0" tween="cubic" easing="out">WindowClose</animation>
				<control type="image">
					<include>Animation_CommonFade</include>
					<posx>60</posx>
					<posy>40</posy>
					<width>759</width>
					<height>682</height>
					<texture background="true">views/listpanel_black.png</texture>
				</control>

				<control type="image">
					<include>Animation_CommonFade</include>
					<posx>60</posx>
					<posy>40</posy>
					<width>759</width>
					<height>682</height>
					<texture>views/listpanel_back.png</texture>
				</control>

				<control type="image">
					<posx>669</posx>
					<posy>49</posy>
					<width>97</width>
					<height>600</height>
					<texture>views/listpanel_detailwide.png</texture>
				</control>
				<control type="group" id="9003">
					<include>Animation_CommonFade</include>
					<include>DefaultCommonEPGView</include> <!-- Default common View id = 3500 -->
					<include>DefaultBigEPGView</include> <!-- Default Big View id = 3510 -->			
				</control>
			</control>
		</control>

		<!-- Left Slide Bar -->
		<control type="group">
			<posy>25</posy>
			<control type="image">
				<posx>-215</posx>
				<posy>190</posy>
				<width>680</width>
				<height>300</height>
				<texture background="true">views/listpanel_black.png</texture>
				<animation effect="rotate" center="auto" start="0" end="-90" time="100" condition="true">Conditional</animation>
				<animation type="Conditional" condition="!ControlGroup(9000).HasFocus">
					<effect type="slide" end="0,280" time="400" easing="in" tween="cubic"/>
				</animation>
				<animation type="WindowClose">
					<effect type="slide" end="0,280" time="400" easing="in" tween="cubic"/>
				</animation>					
			</control>
			<control type="image">
				<posx>-215</posx>
				<posy>190</posy>
				<width>680</width>
				<height>300</height>
				<texture background="true">views/listpanel_back_noreflect.png</texture>
				<animation effect="rotate" center="auto" start="0" end="-90" time="100" condition="true">Conditional</animation>
				<animation type="Conditional" condition="!ControlGroup(9000).HasFocus">
					<effect type="slide" end="0,280" time="400" easing="in" tween="cubic"/>
				</animation>
				<animation type="WindowClose">
					<effect type="slide" end="0,280" time="400" easing="in" tween="cubic"/>
				</animation>
			</control>
			<control type="grouplist" id="9000">
				<animation type="Conditional" condition="!ControlGroup(9000).HasFocus">
					<effect type="slide" end="-280,0" time="400" easing="in" tween="cubic"/>
				</animation>
				<animation type="WindowClose">
					<effect type="slide" end="-280,0" time="400" easing="in" tween="cubic"/>
				</animation>
				<posx>0</posx>
				<posy>25</posy>
				<width>300</width>
				<height>650</height>
				<itemgap>0</itemgap>
				<orientation>vertical</orientation>
				<scroll>true</scroll>
				<onleft>9003</onleft>
				<onright>9003</onright>
				<onup>9000</onup>
				<ondown>9000</ondown>

				<control type="button" id="1111">
					<height>26</height>
					<width>265</width>
					<texturenofocus>-</texturenofocus>
					<texturefocus>settings/listselect.png</texturefocus>
					<alttexturenofocus>-</alttexturenofocus>
					<alttexturefocus>settings/listselect.png</alttexturefocus>

					<textcolor>selected</textcolor>

					<disabledcolor>selected</disabledcolor>
					<textoffsetx>10</textoffsetx>
					<textoffsety>1</textoffsety>
					<align>center</align>
					<aligny>top</aligny>
					<font>Font_Reg19</font>
					<include>Animation_CommonFade</include>
					<enable>no</enable>
					<label>Select view type</label>
				</control>
				<control type="button" id="100">
					<include>Objects_MediaMenuButtonAlt</include>
					<label>VIEW:</label>
				</control>
				<control type="radiobutton" id="101">
					<description>Extra Options</description>
					<include>Objects_MediaMenuButtonAlt</include>
					<label>31026</label>
				</control>
			</control>
		</control>
		<include>DefaultMiniTitle</include>

		<!-- Right smoll Box -->
		<control type="group">
			<include>Animation_CommonFade</include>
			<posx>30</posx>
			<posy>0</posy>
			<control type="group">
				<include>Animation_ListSlideRight</include>
				<control type="image">
					<posx>830</posx>
					<posy>380</posy>
					<width>360</width>
					<height>315</height>
					<texture background="true">views/listpanel_black.png</texture>
				</control>
				<control type="image">
					<posx>830</posx>
					<posy>380</posy>
					<width>360</width>
					<height>315</height>
					<texture>views/listpanel_back.png</texture>
				</control>
				<control type="image">
					<posx>811</posx>
					<posy>333</posy>
					<width>392</width>
					<height>18</height>
					<texture border="20">settings/settings_panelback.png</texture>
				</control>
				<control type="label" id="401">
					<description>Current Channel</description>			
					<posx>838</posx>
					<posy>329</posy>
					<width>352</width>
					<height>18</height>
					<font>Font_Reg20</font>
				</control>

				<control type="group" id="9020">
					<posx>860</posx>
					<posy>405</posy>

					<control type="label" id="300">
						<posx>0</posx>
						<posy>0</posy>
						<font>Font_Reg20</font>
						<textcolor>grey</textcolor>
						<label>00:00-00:00</label>
					</control>

					<control type="label" id="301">
						<posx>150</posx>
						<posy>0</posy>
						<font>Font_Reg20</font>
						<textcolor>grey</textcolor>
						<label>date</label>
					</control>

					<control type="label" id="302">
						<posx>0</posx>
						<posy>30</posy>
						<font>Font_Reg20</font>
						<textcolor>grey</textcolor>
						<label>200Min</label>
					</control>

					<control type="textbox" id="303">
						<description>EPG infomation</description>			
						<posx>0</posx>
						<posy>60</posy>
						<width>330</width>
					 	<height>160</height>
						<font>Font_Reg20</font>
						<textcolor>grey</textcolor>
						<label>none</label>
					</control>
					
					<!-- component -->
					<control type="group">
						<control type="grouplist">
							<posx>0</posx>
							<posy>230</posy>
							<width>190</width>
							<height>30</height>
							<itemgap>1</itemgap>
							<visible>true</visible>
							<orientation>horizontal</orientation>
							<control type="group" id="310">
								<posx>0</posx>
								<posy>0</posy>
								<width>60</width>
								<height>30</height>
								<visible>substring(Window.Property(HasSubtitle),True)</visible>
								<control type="image">
									<posx>0</posx>
									<posy>0</posy>
									<width>60</width>
									<height>30</height>
									<texture>IconTeletext.png</texture>
								</control>
							</control>
							<control type="group" id="311">
								<posx>55</posx>
								<posy>0</posy>
								<width>65</width>
								<height>30</height>
								<visible>substring(Window.Property(HasDolby),True)</visible>
								<control type="image">
									<posx>0</posx>
									<posy>0</posy>
									<width>65</width>
									<height>30</height>
									<texture>dolbydigital.png</texture>
								</control>
							</control>
							<control type="group" id="312">
								<posx>110</posx>
								<posy>0</posy>
								<width>65</width>
								<height>30</height>
								<visible>substring(Window.Property(HasHD),True)</visible>
								<control type="image">
									<posx>0</posx>
									<posy>0</posy>
									<width>65</width>
									<height>30</height>
									<texture>OverlayHD.png</texture>
								</control>
							</control>
						</control>
					</control>

				</control>
			</control>
		</control>
	</controls>
</window>
