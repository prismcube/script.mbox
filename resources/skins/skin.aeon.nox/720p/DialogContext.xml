<window>
	<defaultcontrol always="true">102</defaultcontrol>
	<onload>SetProperty(ItemHeight,38)</onload>					<!--Input Item Height-->
	<allowoverlay>yes</allowoverlay>
	<include>DefaultDialogOpenZoomEffect</include>
	<controls>
		<control type="group" id="9000">
            <include>DefaultDialogCloseZoomEffect</include>
			<control type="image">
				<description>background top image</description>
				<posx>0</posx>
				<posy>-26</posy>
				<width>370</width>
				<height>26</height>
				<texture>dialogs/context_top.png</texture>
			</control>
			<control type="button" id="103">
				<description>Close Window button</description>
				<visible>flase</visible>
				<posx>260</posx>
				<posy>-24</posy>
				<width>64</width>
				<height>32</height>
				<onup>102</onup>
				<ondown>102</ondown>
				<onleft>102</onleft>
				<onright>102</onright>
				<label>-</label>
				<font>-</font>
				<onclick>PreviousMenu</onclick>
				<texturefocus>confluence/DialogCloseButton-focus.png</texturefocus>
				<texturenofocus>confluence/DialogCloseButton.png</texturenofocus>
			</control>
			<control type="image" id="100">
				<description>background image</description>
				<posx>0</posx>
				<posy>0</posy>
				<width>370</width>
				<texture>dialogs/context_middle.png</texture>
			</control>
			<control type="image" id="101">
				<description>background bottom image</description>
				<posx>-13</posx>
				<posy>25</posy>
				<width>370</width>
				<height>27</height>
				<texture>dialogs/context_bottom.png</texture>
			</control>
			<control type="list" id="102">
				<posx>2</posx>
				<posy>0</posy>
				<width>366</width>
				<height>380</height>						<!--Change ProPerty ItemHeight * 10 -->
				<onup>102</onup>
				<ondown>102</ondown>
				<onleft>103</onleft>
				<onright>103</onright>
				<itemlayout width="366" height="38" >		<!--Change ProPerty ItemHeight-->
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>366</width>
						<height>37</height>					<!--Change ProPerty ItemHeight-->
						<texture border="5">dialogs/context_nofocus.png</texture>
					</control>
					<control type="label">
						<posx>185</posx>
						<posy>0</posy>
						<width>366</width>
						<height>38</height>					<!--Change ProPerty ItemHeight-->
						<font>Font_Reg19</font>
						<align>center</align>
						<aligny>center</aligny>
						<textcolor>abFFFFFF</textcolor>
						<label>$INFO[ListItem.Label]</label>
					</control>		
				</itemlayout>
				<focusedlayout width="366" height="38">		<!--Change ProPerty ItemHeight-->
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>366</width>
						<height>37</height>					<!--Change ProPerty ItemHeight-->
						<texture border="5">dialogs/context_highlight.png</texture>
						<pulseonselect>yes</pulseonselect>
					</control>
					<control type="label">
						<posx>185</posx>
						<posy>0</posy>
						<width>366</width>
						<height>38</height>					<!--Change ProPerty ItemHeight-->
						<font>Font_Reg19</font>
						<align>center</align>
						<aligny>center</aligny>
						<textcolor>FFdfdfdf</textcolor>
						<label>$INFO[ListItem.Label]</label>
						<scroll>true</scroll>
					</control>
				</focusedlayout>
			</control>
		</control>
	</controls>
</window>
