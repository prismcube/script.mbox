<window>
	<allowoverlay>yes</allowoverlay>
	<include>DefaultDialogOpenEffect</include>
	<defaultcontrol always="true">300</defaultcontrol>
	<controls>
		<control type="group">
			<posx>340</posx>
			<posy>195</posy>
			<include>DefaultDialogCloseEffect</include>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>610</width>
				<height>320</height>
				<texture>dialogs/dialog_back.png</texture>
				<colordiffuse>$VAR[DialogColorVar]</colordiffuse>
			</control>
			<control type="image">
				<posx>20</posx>
				<width>570</width>
				<height>46</height>
				<texture>osd/osd_settings_buttonnf.png</texture>
			</control>
			<control type="label" id="10">
				<description>Hidden Label</description>
				<posx>0</posx>
				<posy>0</posy>
				<width>0</width>
				<height>0</height>
				<font>-</font>
				<visible>false</visible>
			</control>
			<control type="label" id="200">
				<description>Title</description>
				<posx>0</posx>
				<posy>3</posy>
				<width>610</width>
				<height>60</height>
				<font>Font_Reg19</font>
				<textcolor>FF6db9e5</textcolor>
				<label>-</label>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>selected</textcolor>
			</control>
			<control type="button" id="301">
				<description>Close</description>
				<posx>550</posx>
				<posy>25</posy>
				<width>20</width>
				<height>20</height>
				<texturefocus>special://skin/media/close.png</texturefocus>
				<texturenofocus>close_nofocus.png</texturenofocus>
				<onleft>300</onleft>
				<onright>300</onright>
				<onup>300</onup>
				<ondown>300</ondown>
			</control>
			<control type="label" id="201">
				<description>Message 1</description>
				<posx>30</posx>
				<posy>200</posy>
				<width>550</width>
				<height>30</height>
				<align>left</align>
				<aligny>center</aligny>
				<label>-</label>
				<haspath>true</haspath>
				<font>Font_Reg19</font>
			</control>
			<control type="textbox" id="202">
				<posx>30</posx>
				<posy>60</posy>
				<width>530</width>
				<height>120</height>
				<visible>true</visible>
				<colordiffuse>FFFFFFFF</colordiffuse>
				<font>Font_Reg14</font>
				<textcolor>grey3</textcolor>
				<wrapmultiline>true</wrapmultiline>
				<label>$INFO[Window.Property(ShellDescription)]</label>
			</control>
			<control type="progress" id="400">
				<posx>30</posx>
				<posy>228</posy>
				<width>550</width>
				<height>16</height>
			</control>
			<control type="button" id="300">
				<description>Cancel</description>
				<posx>215</posx>
				<posy>250</posy>
				<width>180</width>
				<height>35</height>
				<label>222</label>
                <colordiffuse>$VAR[ColorDiffuseVar]</colordiffuse>
                <font>Font_Reg19</font>
                <textcolor>grey2</textcolor>
                <focusedcolor>white3</focusedcolor>
                <align>center</align>
				<textoffsetx>30</textoffsetx>
				<onleft>61</onleft>
				<onright>61</onright>
				<onup>301</onup>
				<ondown>301</ondown>
			</control>
		</control>
		<control type="label">
			<posx>390</posx>
			<posy>330</posy>
			<width>500</width>
			<height>50</height>
			<font>Font_Reg25</font>
			<align>center</align>
			<aligny>center</aligny>
			<textcolor>grey2</textcolor>
			<label>$INFO[Window.Property(ShowStatusLabel)]</label>
			<include>DefaultWindowOpenCloseAnimation</include>
			<visible>substring(Window.Property(StatusLabel),true)</visible>
		</control>
	</controls>
</window>
