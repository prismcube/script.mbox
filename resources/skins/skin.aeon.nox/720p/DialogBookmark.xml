<window>
	<defaultcontrol always="true">3850</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<include>DefaultDialogOpenEffect</include>
	<controls>
		<control type="group">
			<include>DefaultDialogCloseEffect</include>
			<posx>350</posx>
			<posy>50</posy>
			<control type="image">
				<description>Panel</description>
				<posx>0</posx>
				<posy>50</posy>
				<width>601</width>
				<height>531</height>
				<texture>osd/osd_settings_back.png</texture>
			</control>
			<control type="image">
				<posx>0</posx>
				<posy>85</posy>
				<width>601</width>
				<height>40</height>
				<texture>osd/osd_settings_buttonnf.png</texture>
			</control>
			<control type="button" id="3800">
				<description>Cancel Window button</description>
				<posx>489</posx>
				<posy>80</posy>
				<width>210</width>
				<height>40</height>
				<textcolor>grey2</textcolor>
				<texturenofocus></texturenofocus>
				<texturefocus></texturefocus>
				<focusedcolor>white</focusedcolor>
				<shadowcolor>black</shadowcolor>
				<label>SCRIPT21</label>
				<font>Font_Bold18</font>
				<onleft>3851</onleft>
				<onright>3802</onright>
				<onup>3802</onup>
				<ondown>3850</ondown>
			</control>
			<control type="label" id="3801">
				<description>header label</description>
				<posx>60</posx>
				<posy>80</posy>
				<width>400</width>
				<height>40</height>
				<font>Font_Bold19</font>
				<textcolor>grey2</textcolor>
				<align>left</align>
				<aligny>center</aligny>
			</control>
			<control type="label" id="3805">
				<posx>0</posx>
				<posy>133</posy>
				<width>600</width>
				<height>30</height>
				<font>Font_Bold19</font>
				<label>$INFO[Control.GetLabel]</label>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>grey2</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<control type="label" id="3803">
				<textcolor>grey2</textcolor>
				<label>$INFO[Control.GetLabel]</label>
				<visible>false</visible>
			</control>
			<control type="label">
				<posx>560</posx>
				<posy>525</posy>
				<width>210</width>
				<height>40</height>
				<font>Font_Reg14</font>
				<align>right</align>
				<aligny>center</aligny>
				<scroll>true</scroll>
				<textcolor>grey2</textcolor>
				<label>$INFO[Control.GetLabel(3803)] SCRIPT106 - SCRIPT107 ($INFO[Container(3850).CurrentPage]/$INFO[Container(3850).NumPages])</label>
				<visible>Control.IsVisible(3850)</visible>
			</control>
			<control type="button" id="3802">
				<description>ok button</description>
				<posx>30</posx>
				<posy>525</posy>
				<width>100</width>
				<height>40</height>
				<align>center</align>
				<font>Font_Bold19</font>
				<label>SCRIPT22</label>
				<textcolor>grey2</textcolor>
				<texturenofocus></texturenofocus>
				<texturefocus></texturefocus>
				<focusedcolor>white</focusedcolor>
				<shadowcolor>black</shadowcolor>
				<onleft>3851</onleft>
				<onright>3800</onright>
				<onup>3850</onup>
				<ondown>3800</ondown>
			</control>
			<control type="list" id="3850">
				<posx>30</posx>
				<posy>175</posy>
				<width>581</width>
				<height>350</height>
				<onleft>3851</onleft>
				<onright>3802</onright>
				<onup>3850</onup>
				<ondown>3850</ondown>
				<pagecontrol>3851</pagecontrol>
				<scrolltime>200</scrolltime>
				<focusposition>0</focusposition>
				<movement>2</movement>
				<animation effect="slide" start="0,0" end="10,0" time="0" condition="!Control.IsVisible(3851)">Conditional</animation>
				<itemlayout height="70" width="581">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>531</width>
						<height>70</height>
						<texture background="true">osd/osd_settings_buttonnf.png</texture>
					</control>
					<!-- Thumbnail  and  Lock -->
					<control type="image">
						<posx>25</posx>
						<posy>5</posy>
						<width>112</width>
						<height>63</height>
						<texture>$INFO[ListItem.Property(RecIcon)]</texture>
					</control>
					<!-- is mark? -->
					<control type="image">
						<posx>20</posx>
						<posy>10</posy>
						<width>20</width>
						<height>20</height>
						<texture>OverlayWatched.png</texture>
						<visible>substring(ListItem.Property(iMark),true)</visible>
					</control>
					<!-- bookmark offset-->
					<control type="label">
						<posx>230</posx>
						<posy>0</posy>
						<width>200</width>
						<height>30</height>
						<font>Font_Bold17</font>
						<textcolor>grey2</textcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<!-- Total -->
					<control type="label">
						<posx>315</posx>
						<posy>0</posy>
						<width>200</width>
						<height>30</height>
						<font>Font_Bold17</font>
						<textcolor>grey2</textcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>/ $INFO[ListItem.Property(RecTotal)]</label>
					</control>
					<control type="progress">
						<posx>160</posx>
						<posy>45</posy>
						<width>340</width>
						<height>18</height>
						<texturebg>osd/seek/progress_back.png</texturebg>
						<lefttexture>osd/seek/progress_left.png</lefttexture>
						<midtexture>osd/seek/progress_mid.png</midtexture>
						<righttexture>osd/seek/progress_right.png</righttexture>
						<progress>percent</progress>
						<visible>substring(ListItem.Property(iPos),true)</visible>
					</control>
					<control type="label">
						<posx>360</posx>
						<posy>0</posy>
						<width>260</width>
						<height>30</height>
						<font>Font_Bold17</font>
						<textcolor>grey2</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Property(RecDate)]</label>
						<animation effect="slide" start="0,0" end="16,0" delay="0" time="0" condition="false">conditional</animation>
					</control>
				</itemlayout>
				<focusedlayout height="70" width="581">
					<control type="image">
						<posx>0</posx>
						<posy>0</posy>
						<width>531</width>
						<height>70</height>
						<texture background="true">osd/osd_settings_buttonf.png</texture>
					</control>
					<!-- Thumbnail  and  Lock -->
					<control type="image">
						<posx>25</posx>
						<posy>5</posy>
						<width>112</width>
						<height>63</height>
						<texture>$INFO[ListItem.Property(RecIcon)]</texture>
					</control>
					<!-- is mark? -->
					<control type="image">
						<posx>20</posx>
						<posy>10</posy>
						<width>20</width>
						<height>20</height>
						<texture>OverlayWatched.png</texture>
						<visible>substring(ListItem.Property(iMark),true)</visible>
					</control>
					<!-- bookmark offset-->
					<control type="label">
						<posx>230</posx>
						<posy>0</posy>
						<width>200</width>
						<height>30</height>
						<font>Font_Bold17</font>
						<textcolor>white</textcolor>
						<selectedcolor>selected</selectedcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Label]</label>
					</control>
					<!-- Total -->
					<control type="label">
						<posx>315</posx>
						<posy>0</posy>
						<width>200</width>
						<height>30</height>
						<font>Font_Bold17</font>
						<textcolor>grey2</textcolor>
						<align>right</align>
						<aligny>center</aligny>
						<label>/ $INFO[ListItem.Property(RecTotal)]</label>
					</control>
					<control type="progress">
						<posx>160</posx>
						<posy>45</posy>
						<width>340</width>
						<height>18</height>
						<texturebg>osd/seek/progress_back.png</texturebg>
						<lefttexture>osd/seek/progress_left.png</lefttexture>
						<midtexture>osd/seek/progress_mid.png</midtexture>
						<righttexture>osd/seek/progress_right.png</righttexture>
						<progress>percent</progress>
						<visible>substring(ListItem.Property(iPos),true)</visible>
					</control>
					<control type="label">
						<posx>360</posx>
						<posy>0</posy>
						<width>260</width>
						<height>30</height>
						<font>Font_Bold17</font>
						<textcolor>grey2</textcolor>
						<align>left</align>
						<aligny>center</aligny>
						<label>$INFO[ListItem.Property(RecDate)]</label>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="3851">
				<posx>20</posx>
				<posy>180</posy>
				<width>28</width>
				<height>340</height>
				<texturesliderbackground>views/scrollbarbacklist.png</texturesliderbackground>
				<texturesliderbar>-</texturesliderbar>
				<texturesliderbarfocus>-</texturesliderbarfocus>
				<textureslidernib>views/scrollbarnub-nofocus.png</textureslidernib>
				<textureslidernibfocus>views/scrollbarnub-focus.png</textureslidernibfocus>
				<onleft>3802</onleft>
				<onright>3850</onright>
				<ondown></ondown>
				<onup></onup>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
			</control>
		</control>
	</controls>
</window>
