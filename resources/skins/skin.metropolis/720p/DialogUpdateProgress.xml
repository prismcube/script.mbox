<window>
	<allowoverlay>yes</allowoverlay>
	<defaultcontrol always="true">300</defaultcontrol>
	<controls>
		<control type="group">
			<posx>340</posx>
			<posy>195</posy>
			<include>DefaultDialogCloseEffect</include>
			<control type="image">
				<description>background image</description>
				<posx>-10</posx>
				<posy>-30</posy>
				<width>630</width>
				<height>370</height>
				<texture>frames/Dialog_Glass2Panel.png</texture>
				<colordiffuse>DialogBackgroundColor</colordiffuse>
			</control>
			<control type="label" id="10">
				<description>fake heading label</description>
				<posx>0</posx>
				<posy>0</posy>
				<width>0</width>
				<height>0</height>
				<font>-</font>
				<visible>false</visible>
			</control>
			<control type="label" id="200">
				<description>Title</description>
				<posx>40</posx>
				<posy>18</posy>
				<width>530</width>
				<height>30</height>
				<font>METF_Menu</font>
				<label>-</label>
				<align>center</align>
				<aligny>center</aligny>
				<textcolor>DialogTextFO</textcolor>
			</control>
			<control type="button" id="301">
				<description>Close Window button</description>
				<posx>555</posx>
				<posy>23</posy>
				<width>25</width>
				<height>25</height>
				<label>-</label>
				<font>-</font>
				<texturefocus>new/exitbuttonFO.png</texturefocus>
				<texturenofocus>new/exitbutton.png</texturenofocus>
				<onleft>300</onleft>
				<onright>300</onright>
				<onup>300</onup>
				<ondown>300</ondown>
			</control>
			<control type="label" id="201">
				<description>dialog line 1</description>
				<posx>30</posx>
				<posy>200</posy>
				<width>550</width>
				<height>30</height>
				<align>left</align>
				<aligny>center</aligny>
				<label>-</label>
				<haspath>true</haspath>
				<font>METF_PlotText</font>
			</control>
			<control type="textbox" id="202">
				<posx>30</posx>
				<posy>60</posy>
				<width>530</width>
				<height>120</height>
				<visible>true</visible>
				<colordiffuse>FFFFFFFF</colordiffuse>
				<font>METF_PlotText</font>
				<textcolor>white</textcolor>
				<wrapmultiline>true</wrapmultiline>
				<!--pagecontrol>61</pagecontrol-->
				<label>$INFO[Window.Property(ShellDescription)]</label>
			</control>
			<control type="progress" id="400">
				<description>Progressbar</description>
				<posx>30</posx>
				<posy>228</posy>
				<width>550</width>
				<height>16</height>
				<texturebg>osd/osd_progress_back2.png</texturebg>
				<lefttexture>osd/osd_progress_left2.png</lefttexture>
				<midtexture>osd/osd_progress_mid2.png</midtexture>
				<righttexture>osd/osd_progress_right2.png</righttexture>
				<colordiffuse>DialogProgressBar</colordiffuse>
			</control>
			<control type="button" id="300">
				<description>Cancel button</description>
				<posx>205</posx>
				<posy>255</posy>
				<width>230</width>
				<height>40</height>
				<label>222</label>
				<font>METF_DialogLargeBold</font>
				<textcolor>white</textcolor>
				<focusedcolor>DialogTextFO</focusedcolor>
				<align>center</align>
				<onleft>61</onleft>
				<onright>61</onright>
				<onup>301</onup>
				<ondown>301</ondown>
			</control>
		</control>
		<control type="label">
			<posx>390</posx>
			<posy>340</posy>
			<width>500</width>
			<height>50</height>
			<font>METF_MusicVizTitle</font>
			<align>center</align>
			<aligny>center</aligny>
			<textcolor>white</textcolor>
			<label>$INFO[Window.Property(ShowStatusLabel)]</label>
			<include>DefaultWindowOpenCloseAnimation</include>
			<visible>substring(Window.Property(StatusLabel),true)</visible>
		</control>
	</controls>
</window>

