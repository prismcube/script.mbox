<window>
	<allowoverlay>yes</allowoverlay>
	<include>DefaultDialogOpenEffect</include>
	<controls>
		<control type="group">
			<posx>340</posx>
			<posy>260</posy>
			<include>DefaultDialogCloseEffect</include>
			<control type="image" id="9001">
				<description>background image</description>
				<posx>0</posx>
				<posy>0</posy>
				<width>600</width>
				<height>160</height>
				<texture border="20">OverlayDialogBackground.png</texture>
			</control>
			<control type="label" id="100">
				<description>dialog line 1</description>
				<posx>20</posx>
				<posy>60</posy>
				<width>560</width>
				<height>30</height>
				<align>left</align>
				<aligny>center</aligny>
				<haspath>true</haspath>
				<font>font13</font>
			</control>
			<control type="label" id="200">
				<description>header label</description>
				<posx>20</posx>
				<posy>18</posy>
				<width>560</width>
				<height>30</height>
				<font>font13_title</font>
				<align>left</align>
				<aligny>center</aligny>
				<textcolor>blue</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>
			<control type="label" id="201">
				<description>page</description>
				<posx>580</posx>
				<posy>18</posy>
				<width>100</width>
				<height>30</height>
				<font>font13</font>
				<align>right</align>
				<aligny>center</aligny>
				<textcolor>white</textcolor>
				<shadowcolor>black</shadowcolor>
			</control>

			<control type="label" id="300">
				<description>dialog line 2</description>
				<posx>20</posx>
				<posy>85</posy>
				<width>560</width>
				<height>30</height>
				<align>left</align>
				<aligny>center</aligny>
				<haspath>true</haspath>
				<font>font13</font>
			</control>
			<control type="progress" id="400">
				<description>Progressbar</description>
				<posx>20</posx>
				<posy>120</posy>
				<width>560</width>
				<height>12</height>
				<texturebg border="6,0,6,0">OSDProgressBack.png</texturebg>
				<midtexture border="6,0,6,0">OSDProgressMid.png</midtexture>
			</control>
		</control>
	</controls>
</window>
